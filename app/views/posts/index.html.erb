<div class="banner ">

  <!-- Modal -->
  <div class="modal custom fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <h1 class="modal-title fs-5" id="exampleModalLabel">Partage ta musique</h1>
          <button type="button" class="btn-close white" data-bs-dismiss="modal" aria-label="Close"></button>
        </div>
        <div class="modal-body">
          <%= render "post_form" %>
        </div>

      </div>
    </div>
  </div>
  <img class="logo-wak mb-2 mt-1" src="https://res.cloudinary.com/dsbtmmjt2/image/upload/v1701966700/akely_oyctua.png"
    alt="logo wakely">
  <div class="scrollmenu mx-3 wakely-font">
    <% Post::MOODS.each do |mood|  %>
    <%=link_to mood, posts_path(mood: mood), class:"a-link" %>
    <% end %>
  </div>
  <div class="container-md">
    <%= render "cardpost"%>
    <br>
    <br>
  </div>
