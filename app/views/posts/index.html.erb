<div class= "banner border border-secondary text-center">
<h1> Liste des Posts</h1>
</div>
<div class="container">
<%= link_to "users", users_path%>
<div class= "post text-center my-5 ">
<div class= "row">
<div class= "col-md-6 offset-md-3">
<% @posts.each do |post| %>
 <div class="card p-3 bg-white mb-2">
<div class="notification d-flex justify-content-between bg-success">
  <div class= "picture bg-secondary d-flex" >
  <img src='data:image/jpeg;base64,' class="avatar-large" />
  <h2><%= link_to post.user.username, user_path(post.user)%><h2>
  </div>
    <p><small>Mood: <img src="data:image/jpeg;base64," alt=""> <%= post.mood%></small></p>
 </div>
  <div class="card mb-3" style="max-width: 540px;">
  <div class="row g-0">
    <div class="col-md-4">
      <img src="data:image/jpeg;base64," class="img-fluid rounded-start" alt="...">
    </div>
    <div class="col-md-8">
      <div class="card-body">
        <h5 class="card-title">Mirza</h5>
        <p class="card-text">Nino Ferrer</p>
        <p class="card-text"><small class="text-muted">Last updated 3 mins ago</small></p>
      </div>
    </div>
  </div>
</div>
    <p>Lorem ipsum dolor sit amet, <strong>consectetur</strong> adipisicing elit.</p>
<%= link_to "see the post", post_path(post)%>
<% end %>
</div>
</div>
</div>
</div>
