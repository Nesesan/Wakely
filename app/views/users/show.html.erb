<div class="container py-3 h-100 mb-2">
  <div class="d-flex justify-content-between mb-3">
    <%= link_to root_path, class: "return-arrow " do %>
    <%= image_tag("logo-retour.png", size: "20x20", class: "responsive-image mt-2 png") %>
    <% end %>
    <%= link_to destroy_user_session_path, data: {turbo_method: :delete} do %>
    <button class="Btn">
      <div class="sign"><svg viewBox="0 0 512 512">
          <path
            d="M377.9 105.9L500.7 228.7c7.2 7.2 11.3 17.1 11.3 27.3s-4.1 20.1-11.3 27.3L377.9 406.1c-6.4 6.4-15 9.9-24 9.9c-18.7 0-33.9-15.2-33.9-33.9l0-62.1-128 0c-17.7 0-32-14.3-32-32l0-64c0-17.7 14.3-32 32-32l128 0 0-62.1c0-18.7 15.2-33.9 33.9-33.9c9 0 17.6 3.6 24 9.9zM160 96L96 96c-17.7 0-32 14.3-32 32l0 256c0 17.7 14.3 32 32 32l64 0c17.7 0 32 14.3 32 32s-14.3 32-32 32l-64 0c-53 0-96-43-96-96L0 128C0 75 43 32 96 32l64 0c17.7 0 32 14.3 32 32s-14.3 32-32 32z">
          </path>
        </svg></div>
      <div class="text">Logout</div>
    </button>
    <%end%>
  </div>
  <div class="row d-flex justify-content-center align-items-center h-100">
    <div class="col-md-12 col-xl-4">
      <div class="card">
        <div class="card-body text-center">
          <div class="mt-2 mb-4">
            <img
              src="data:image/png;base64,"
              class="rounded-circle img-fluid" style="width: 100px;" />
          </div>
          <h4 class="wakely-font mb-2"><%= @user.username %></h4>
        </div>
      </div>
    </div>
  </div>
  <div class="row d-flex justify-content-center align-items-center h-100 mt-3">
    <div class="col-md-12 col-xl-4">
      <h6 class="wakely-font justify-content-center d-flex"><strong>Dernier post</strong></h6>
      <div class="card ">
        <div class="card-body text-center">
          <%=@user.last_post_day %>
        </div>
      </div>
    </div>
    <div class="row d-flex justify-content-center align-items-center h-100 mt-3">
      <div class="col-md-12 col-xl-4">
        <h6 class=" wakely-font justify-content-center d-flex"><strong>Memories</strong></h6>
        <div class="card">
          <div class="card-body text-center">
            <% @last_posts.each do |post| %>
            <div class="post">
              <div class="title-post">
                <div class="profil-img-username d-flex flex-row align-items-end">
                  <img
                    src="https://yt3.googleusercontent.com/6qGEhp2vSYQm_n0Xaby-5rgr1kW9ldtMSPafl3FTXOlYzQxaZ15Dp7DRSNNVWN3upuor1qV68Lo=s900-c-k-c0x00ffffff-no-rj"
                    class="img-profil" alt="sad">
                </div>
                <div class="profil-info">
                  <div class="user text-start">
                    <%= link_to post.user.username, user_path(post.user), class:"text-decoration-none text-dark"%>
                  </div>
                  <div class="date-time ">
                    <small><%=post.created_at%></small>
                  </div>
                </div>
              </div>
              <div class="main-post">
                <div class="track">
                  <div class="track-picture mx-2">
                    <img src="https://upload.chien.com/img_global/24-culture/_medium-18448_9860-chanson-chien.jpg"
                      class="img" alt="">
                  </div>
                  <div class="track-info mx-2">
                    <h5><strong>Mirza</strong></h5>
                    <h6>Nino Ferrerorocher</h6>
                  </div>
                  <div class="mood">
                    <h4>Mood: Sad</h4>
                  </div>
                </div>
                <div class="content mx-3">
                  <p>Fid√®le au poste d'observation</p>
                </div>
              </div>
            </div>
            <% end %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
