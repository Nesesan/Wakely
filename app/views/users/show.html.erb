<div class="w-10 col-2 justify-content-center d-flex mt-3">
  <%= link_to root_path, class: "return-arrow" do %>
  <%= image_tag("https://cdn0.iconfinder.com/data/icons/entypo/46/chevron4-1024.png", size: "20x20", class: "responsive-image") %>
  <% end %>
</div>
<div class="container py-3 h-100 mb-2">
  <div class="row d-flex justify-content-center align-items-center h-100">
    <div class="col-md-12 col-xl-4">
      <div class="card">
        <div class="card-body text-center">
          <div class="mt-3 mb-4">
            <img
              src="data:image/png;base64,"
              class="rounded-circle img-fluid" style="width: 100px;" />
          </div>
          <h4 class="wakely-font mb-2"><%= @user.username %></h4>
        </div>
      </div>
    </div>
  </div>
  <div class="row d-flex justify-content-center align-items-center h-100 mt-3">
    <div class="col-md-12 col-xl-4">
      <h6 class="wakely-font justify-content-center d-flex"><strong>Mood du jour</strong></h6>
      <div class="card">
        <div class="card-body text-center">
          <%=@user.last_mood %>
        </div>
      </div>
    </div>
    <div class="row d-flex justify-content-center align-items-center h-100 mt-3">
      <div class="col-md-12 col-xl-4">
        <h6 class="wakely-font justify-content-center d-flex"><strong>Memories</strong></h6>
        <div class="card">
          <div class="card-body text-center">
            <% @last_posts.each do |post| %>
            <li><%= post.mood %></li>
            <% end %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
